<p id="notice"><%= notice %></p>

<h1>Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>Unit module</th>
      <th>User</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.unit_module_id %></td>
        <td><%= enrollment.user_id %></td>
        <td><%= enrollment.status_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

